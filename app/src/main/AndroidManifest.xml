        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".customer.activities.BoxesActivity">

        </activity>
        <activity android:name=".gen_activities.RegisterActivity"></activity>
        <activity
